{
  "expo": {
    "name": "HagzApp",
    "slug": "HagzApp",
    "version": "16",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#4CAF50"
    },
    "newArchEnabled": true,
    "jsEngine": "hermes",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/icon.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundImage": "./assets/icon.png",
        "backgroundColor": "#4CAF50"
      },
      "icon": "./assets/icon.png",
      "package": "com.saadany3.hagzapp2024",
      "versionCode": 16,
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO",
        "android.permission.INTERNET",
        "android.permission.WAKE_LOCK",
        "android.permission.RECEIVE_BOOT_COMPLETED"
      ]
    },
    "web": {
      "favicon": "./assets/icon.png",
      "bundler": "metro"
    },
    "experiments": {
      "typedRoutes": true
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you select profile pictures and share images in the app.",
          "cameraPermission": "The app accesses your camera to let you take profile pictures and share photos in the app."
        }
      ],
      [
        "@react-native-firebase/app",
        {
          "android": {
            "googleServicesFile": "./google-services.json"
          }
        }
      ],
      [
        "@react-native-firebase/messaging",
        {
          "android": {
            "requestPermission": false
          }
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/notification_icon.png",
          "color": "#4CAF50",
          "defaultChannel": "default",
          "sounds": [
            "./assets/notification_sound.wav"
          ]
        }
      ],
      "./plugins/withFirebaseManifestFix.js"
    ],
    "extra": {
      "eas": {
        "projectId": "09cc7470-3c53-4046-9782-ff80cf1e5b2c"
      }
    },
    "owner": "saadany3"
  }
}
